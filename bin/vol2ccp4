#!/usr/bin/python
import sys, os
inp = sys.argv[1]
outp = sys.argv[2]
sourcedir = os.path.split(sys.argv[0])[0]
map2map = sourcedir + os.sep + "map2map"
script = """
tmpfile=`mktemp`
tmpfile2=`mktemp`
%(map2map)s %(inp)s $tmpfile << _Eod_
 10
_Eod_
%(map2map)s $tmpfile $tmpfile2 << _Eod_
 5
_Eod_
%(map2map)s $tmpfile2 %(outp)s << _Eod_
 7
_Eod_
rm -f tmpfile tmpfile2
""" % {"inp":inp,"outp":outp,"map2map":map2map}
os.system(script)
